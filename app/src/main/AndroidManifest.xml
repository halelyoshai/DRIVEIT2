<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.driveit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DRIVEIT">
        <activity android:name=".TeacherProfile_Activity"></activity>
        <activity android:name=".SignupStudent_Activity" />
        <activity android:name=".SchedulingActivity" />
        <activity android:name=".StudentslistActivity_T" />
        <activity android:name=".Info_Activity" />
        <activity android:name=".RatingActivity_S" />
        <activity android:name=".RatingActivity_T" />
        <activity android:name=".SignupTeacher_Activity" />
        <activity android:name=".StudentsProfile_Activity" />
        <activity android:name=".SigninActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>